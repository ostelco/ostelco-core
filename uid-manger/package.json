{
  "name": "uid-manger",
  "version": "0.0.1",
  "description": "UserId manager for panacea",
  "main": "dist/server.js",
  "engines": {
    "node": ">=9.6",
    "npm": ">=5.6"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.3.4",
    "@google-cloud/debug-agent": "^2.3.2",
    "date-fns": "^1.29.0",
    "express": "^4.16.2",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/google-cloud__datastore": "^1.3.0",
    "@types/jest": "^22.2.0",
    "@types/node": "^9.4.6",
    "@types/uuid": "^3.4.3",
    "google-datastore-emulator": "^1.5.0",
    "jest": "^22.4.2",
    "prettier": "^1.11.0",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "build": "npm run tslint && npm run build-ts",
    "build-ts": "tsc",
    "deploy": "npm run build && gcloud app deploy --verbosity=info",
    "prettier": "prettier --parser typescript --print-width 100 --write \"./**/*.ts\"",
    "serve": "node dist/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "author": "Prasanth Ullattil",
  "license": "ISC"
}
