{
  "name": "uid-manger",
  "version": "0.0.1",
  "description": "UserId manager for panacea",
  "main": "dist/server.js",
  "engines": {
    "node": ">=9.6",
    "npm": ">=5.6"
  },
  "dependencies": {
    "@google-cloud/datastore": "^1.3.4",
    "express": "^4.16.2",
    "got": "^8.2.0",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "@types/express": "^4.11.1",
    "@types/google-cloud__datastore": "^1.3.0",
    "@types/got": "^7.1.7",
    "@types/node": "^9.4.6",
    "@types/uuid": "^3.4.3",
    "prettier": "^1.11.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.9.0",
    "tslint-plugin-prettier": "^1.3.0",
    "typescript": "^2.7.2"
  },
  "scripts": {
